{
	"name": "fortify-formatter",
	"displayName": "Fortify Formatter",
	"version": "1.0.1",
	"publisher": "Inc44",
	"description": "A formatter for various languages with specific configurations.",
	"author":
	{
		"name": "Inc44"
	},
	"categories": ["Formatters"],
	"icon": "fortify.png",
	"pricing": "Free",
	"activationEvents": ["onStartupFinished"],
	"engines":
	{
		"vscode": "^1.60.0"
	},
	"main": "./extension.js",
	"scripts":
	{
		"package": "vsce package"
	},
	"devDependencies":
	{
		"@types/vscode": "^1.60.0",
		"@types/node": "^22.13.10",
		"@vscode/vsce": "^3.2.2"
	},
	"license": "SEE LICENSE IN LICENSE",
	"repository":
	{
		"type": "git",
		"url": "https://github.com/Inc44/Fortify.git"
	},
	"homepage": "https://github.com/Inc44/Fortify/blob/master/README.md",
	"dependencies":
	{
		"@prettier/plugin-php": "^0.19.0",
		"@wasm-fmt/clang-format": "0.9.7",
		"@wasm-fmt/gofmt": "0.4.9",
		"@wasm-fmt/lua_fmt": "0.1.0",
		"@wasm-fmt/ruff_fmt": "0.9.7",
		"@wasm-fmt/sql_fmt": "0.1.0",
		"@wasm-fmt/zig_fmt": "0.0.5",
		"js-beautify": "^1.15.4",
		"prettier": "^2.8.8"
	},
	"contributes":
	{
		"commands": [
		{
			"command": "fortifyFormatter.formatDocument",
			"title": "Format with Fortify Formatter"
		}],
		"menus":
		{
			"editor/context": [
			{
				"command": "fortifyFormatter.formatDocument",
				"group": "1_modification",
				"when": "editorHasDocumentFormattingProvider && (editorLangId == c || editorLangId == cpp || editorLangId == css || editorLangId == go || editorLangId == html || editorLangId == javascript || editorLangId == json || editorLangId == lua || editorLangId == php || editorLangId == python || editorLangId == sql || editorLangId == zig)"
			}]
		},
		"configuration":
		{
			"title": "Fortify Formatter"
		}
	}
}